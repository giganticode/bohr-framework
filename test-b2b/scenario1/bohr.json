{
  "bohr_framework_version": "0.4.4-rc",
  "tasks": {
    "bugginess-1k": {
      "top_artifact": "bohr.artifacts.commit.Commit",
      "label_categories": [
        "CommitLabel.NonBugFix",
        "CommitLabel.BugFix"
      ],
      "test_datasets": [
        "1151-commits",
        "berger"
      ],
      "train_datasets": [
        "200k-commits-1k"
      ],
      "label_column_name": "bug"
    },
    "smells_10": {
      "description": "desc",
      "top_artifact": "bohr.artifacts.method.Method",
      "label_categories": [
        "SnippetLabel.NonSmelly",
        "SnippetLabel.Smelly"
      ],
      "test_datasets": [
        "smells-test_10"
      ],
      "train_datasets": [
        "smells-train_10"
      ],
      "label_column_name": "smelly"
    }
  },
  "datasets": {
    "berger": {
      "description": "desc",
      "path": "berger.csv",
      "mapper": "bohr.templates.datamappers.commit.CommitMapper",
      "test_set": "true",
      "preprocessor": "copy",
      "loader": "csv"
    },
    "1151-commits": {
      "description": "desc",
      "path": "1151-commits.csv",
      "mapper": "bohr.templates.datamappers.commit.CommitMapper",
      "test_set": "true",
      "preprocessor": "copy",
      "loader": "csv"
    },
    "200k-commits-1k": {
      "description": "desc",
      "path": "200k-commits.csv",
      "mapper": "bohr.templates.datamappers.commit.CommitMapper",
      "test_set": "false",
      "loader": "csv",
      "preprocessor": "copy",
      "n_rows": 1000
    },
    "200k-commits-issues": {
      "description": "desc",
      "path": "200k-commits-issues.csv",
      "mapper": "bohr.templates.datamappers.issue.IssueMapper",
      "test_set": "false",
      "loader": "csv",
      "preprocessor": "copy"
    },
    "200k-commits-files": {
      "description": "desc",
      "path": "200k-commits-files.csv.7z",
      "mapper": "bohr.templates.datamappers.commitfile.CommitFileMapper",
      "test_set": "false",
      "loader": "csv",
      "preprocessor": "7z"
    },
    "smells-train_10": {
      "description": "desc",
      "path": "smells-madeyski.csv",
      "path_preprocessed": "smells/train.csv",
      "mapper": "bohr.templates.datamappers.method.MethodMapper",
      "test_set": "false",
      "loader": "csv",
      "preprocessor": "data-preprocessing/smells.sh",
      "n_rows": 10,
      "sep": ";"
    },
    "smells-test_10": {
      "path": "smells-madeyski.csv",
      "path_preprocessed": "smells/test.csv",
      "mapper": "bohr.templates.datamappers.method.MethodMapper",
      "test_set": "false",
      "loader": "csv",
      "preprocessor": "data-preprocessing/smells.sh",
      "n_rows": 10,
      "sep": ";"
    }
  },
  "dataset-linkers": [
    {
      "from": "200k-commits-1k",
      "to": "200k-commits-issues",
      "link_file": "commit_to_issues.csv",
      "linker": "bohr.linkers.l.CommitIssueLinker"
    },
    {
      "from": "200k-commits-1k",
      "to": "200k-commits-files",
      "link_file": "commit_to_files.csv",
      "linker": "bohr.linkers.l.CommitFileLinker"
    }
  ]
}
